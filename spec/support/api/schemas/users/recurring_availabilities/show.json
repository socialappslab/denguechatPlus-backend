{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "additionalProperties": false,
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "$id": "#/properties/data",
      "type": "array",
      "title": "The Data Schema",
      "additionalProperties": false,
      "items": {
        "$id": "#/properties/data/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "id",
          "type",
          "attributes"
        ],
        "properties": {
          "id": {
            "$id": "#/properties/data/items/properties/id",
            "type": "string",
            "title": "The Id Schema",
            "default": "",
            "examples": [
              "21"
            ],
            "pattern": "^(.*)$"
          },
          "type": {
            "$id": "#/properties/data/items/properties/type",
            "type": "string",
            "title": "The Type Schema",
            "default": "",
            "examples": [
              "recurring-availabilities"
            ],
            "pattern": "^(.*)$"
          },
          "attributes": {
            "$id": "#/properties/data/items/properties/attributes",
            "type": "object",
            "title": "The Attributes Schema",
            "additionalProperties": false,
            "required": [
              "day-of-week",
              "start",
              "duration"
            ],
            "properties": {
              "day-of-week": {
                "$id": "#/properties/data/items/properties/attributes/properties/day-of-week",
                "type": "string",
                "title": "The Day-of-week Schema",
                "default": "",
                "examples": [
                  "tuesday"
                ],
                "pattern": "^(.*)$"
              },
              "start": {
                "$id": "#/properties/data/items/properties/attributes/properties/start",
                "type": "integer",
                "title": "The Start Schema",
                "default": 0,
                "examples": [
                  65
                ]
              },
              "duration": {
                "$id": "#/properties/data/items/properties/attributes/properties/duration",
                "type": "integer",
                "title": "The Duration Schema",
                "default": 0,
                "examples": [
                  9
                ]
              }
            }
          }
        }
      }
    }
  }
}
