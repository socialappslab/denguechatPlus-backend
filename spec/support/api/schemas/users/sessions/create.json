{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "data",
    "meta"
  ],
  "properties": {
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "required": [
        "id",
        "type",
        "attributes"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/data/properties/id",
          "type": "string"
        },
        "type": {
          "$id": "#/properties/data/properties/type",
          "type": "string"
        },
        "attributes": {
          "$id": "#/properties/data/properties/attributes",
          "type": "object",
          "required": [
            "first-name",
            "last-name",
            "email",
            "role-name",
            "timezone",
            "onboarding-status",
            "reset-2fa"
          ],
          "properties": {
            "first-name": {
              "$id": "#/properties/data/properties/attributes/properties/first-name",
              "type": "string"
            },
            "last-name": {
              "$id": "#/properties/data/properties/attributes/properties/last-name",
              "type": "string"
            },
            "email": {
              "$id": "#/properties/data/properties/attributes/properties/email",
              "type": "string"
            },
            "role-name": {
              "$id": "#/properties/data/properties/attributes/properties/role-name",
              "type": "string"
            },
            "timezone": {
              "$id": "#/properties/data/properties/attributes/properties/timezone",
              "type": "string"
            },
            "onboarding-status": {
              "$id": "#/properties/data/properties/attributes/properties/onboarding-status",
              "type": "string"
            },
            "reset-2fa": {
              "$id": "#/properties/data/properties/attributes/properties/reset-2fa",
              "type": "boolean"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    },
    "meta": {
      "$id": "#/properties/meta",
      "type": "object",
      "required": [
        "jwt"
      ],
      "properties": {
        "jwt": {
          "$id": "#/properties/meta/properties/jwt",
          "type": "object",
          "required": [
            "csrf",
            "access",
            "access_expires_at",
            "refresh",
            "refresh_expires_at"
          ],
          "properties": {
            "csrf": {
              "$id": "#/properties/meta/properties/jwt/properties/csrf",
              "type": "string"
            },
            "access": {
              "$id": "#/properties/meta/properties/jwt/properties/access",
              "type": "string"
            },
            "access_expires_at": {
              "$id": "#/properties/meta/properties/jwt/properties/access_expires_at",
              "type": "string"
            },
            "refresh": {
              "$id": "#/properties/meta/properties/jwt/properties/refresh",
              "type": "string"
            },
            "refresh_expires_at": {
              "$id": "#/properties/meta/properties/jwt/properties/refresh_expires_at",
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
